#include <iostream>
using namespace std;

int main() {
    int T;
    cin >> T; 
    
    for (int i = 0; i < T; i++) {
        int X;
        cin >> X;  

        
        if (X + 3 <= 10) {
            cout << "Yes" << endl;
        } else {
            cout << "No" << endl;
        }
    }

    return 0;
}
