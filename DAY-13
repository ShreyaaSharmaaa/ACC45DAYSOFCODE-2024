#include <iostream>
using namespace std;

int main() {
    int T;
    cin >> T;
    
    
    for (int i = 0; i < T; i++) {
        int X, Y;
        cin >> X >> Y;  
       
        if ( (Y*100/X) >= 50) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    }
    
    return 0;
}
